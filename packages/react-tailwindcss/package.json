{
  "name": "@universal-labs/react-tailwindcss",
  "version": "1.0.0",
  "description": "Tailwindcss Inline StyleSheet generator for react native",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "react-native",
    "react"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/react-tailwindcss"
  },
  "exports": {
    ".": {
      "import": "./build/index.js",
      "require": "./build/index.js",
      "browser": "./build/index.js",
      "types": "./build/index.d.ts"
    },
    "./primitives": {
      "import": "./build/primitives.js",
      "require": "./build/primitives.js",
      "browser": "./build/primitives.js",
      "types": "./build/primitives.d.ts"
    }
  },
  "main": "./build/index.js",
  "files": [
    "src",
    "build"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "prepare": "yarn build"
  },
  "dependencies": {
    "@react-universal/core": "^1.3.0",
    "@react-universal/primitives": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/tailwindcss": "3.1.0",
    "tailwindcss": "3.2.4"
  },
  "peerDependencies": {
    "postcss": ">= 8.0.0",
    "tailwindcss": ">= 3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "a5c05d5ee9bc016aa01fc3a7cc03bddd46d1e634"
}
