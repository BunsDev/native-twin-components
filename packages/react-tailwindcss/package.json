{
  "name": "@universal-labs/react-tailwindcss",
  "version": "1.0.0",
  "description": "Tailwindcss Inline StyleSheet generator for react native",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "react-native",
    "react"
  ],
  "homepage": "https://github.com/react-universal/tailwind#readme",
  "bugs": {
    "url": "https://github.com/react-universal/tailwind/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/react-universal/tailwind",
    "directory": "packages/react-tailwindcss"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/index.es.js",
      "require": "./build/index.cjs.js",
      "browser": "./build/index.umd.js",
      "types": "./index.d.ts"
    }
  },
  "main": "./build/index.cjs.js",
  "unpkg": "./build/index.iife.js",
  "module": "./build/index.cjs.js",
  "types": "./index.d.ts",
  "files": [
    "src",
    "build",
    "index.d.ts",
    "types.d.ts"
  ],
  "scripts": {
    "build": "yarn vite build",
    "dev": "yarn vite build -w",
    "prepare": "yarn build",
    "test:ui": "vitest --ui"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@react-universal/core": "^1.3.0",
    "@react-universal/primitives": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/react": "~18.0.27",
    "@types/react-dom": "~18.0.10",
    "@types/tailwindcss": "3.1.0",
    "tailwindcss": "3.2.4",
    "vite": "^4.1.1"
  },
  "peerDependencies": {
    "postcss": ">= 8.0.0",
    "tailwindcss": ">= 3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "gitHead": "a5c05d5ee9bc016aa01fc3a7cc03bddd46d1e634"
}
